{
  "editor.fontFamily": "JetBrainsMono Nerd Font, D2Coding",
  "editor.fontSize": 14,
  "editor.letterSpacing": 0.4,
  "editor.renderWhitespace": "boundary",
  "editor.renderFinalNewline": true,
  "editor.renderControlCharacters": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.rulers": [80],
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "files.maxMemoryForLargeFilesMB": 8192,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "newUntitledFile",
  "window.menuBarVisibility": "hidden",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.tsserver.maxTsServerMemory": 8192,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.inlayHints.variableTypes.enabled": true,
  "vim.hlsearch": true,
  "vim.ignorecase": false,
  "vim.handleKeys": {
    "<C-d>": false
  },
  "todo-tree.regex.regex": "( \\*|//|#|<!--|;|/\\*|^|^[ \\t]*(-|\\d+.))\\s*($TAGS)",
  "todo-tree.highlights.defaultHighlight": {
    "background": "#ad7d02",
    "foreground": "#000",
    "iconColour": "#0f0",
    "icon": "check",
    "gutterIcon": true
  },
  "todo-tree.highlights.customHighlight": {
    "FIXME": {
      "background": "#8f0000",
      "iconColour": "#f00"
    }
  },
  "gitlens.defaultDateStyle": "absolute",
  "gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm",
  "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
  "gitlens.defaultTimeFormat": "HH:mm",
  "path-intellisense.showHiddenFiles": true,
  "python.formatting.provider": "autopep8",
  "python.linting.pylintEnabled": true,
  "python.analysis.typeCheckingMode": "basic",
  "python.venvFolders": [".local/share/python/virtualenvs"],
  "livePreview.openPreviewTarget": "External Browser",
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "[c]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[rust]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "matklad.rust-analyzer"
  },
  "[go]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": false,
    "editor.defaultFormatter": "golang.go"
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-python.python"
  },
  "[sql]": {
    "editor.defaultFormatter": "lea21st.vscode-sql-formatter"
  },
  "[markdown]": {
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.wordWrap": "on",
    "editor.quickSuggestions": false,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[dockercompose]": {
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "sql-formatter.dialect": "mariadb"
}
